
var inputVariable = 0;

function getThree() {
    var localVar = 3;
    return localVar;
}
procedure initNSeven() {
    var localVar = getThree() * 2 + 1;
    inputVariable = localVar;
}
function fact(n) {
    if (n >= 0) {
        if (n == 0) {
            return 1;
        }
        return n * fact(n - 1);
    } else {
        return -1;
    }
}


initNSeven();
return fact(inputVariable);